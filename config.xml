<widget id="com.intranet" version="0.0.1">
	<name>Intranet</name>
	<description>
		Intranet Professional Air.
	</description>
	<author email="desarrollo1@professionalair.edu.pe" href="http://www.professionalair.edu.pe">
		Professional Air Sistemas.
	</author>

	<content src="index.html" />

	<plugin name="cordova-plugin-whitelist" />
	<access origin="*" launch-external="yes" />

	<preference name="Orientation" value="portrait" />
	<preference name="Fullscreen" value="true" />
</widget>

